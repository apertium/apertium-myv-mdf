      </program>
      <program name="apertium-postchunk">
        <file name="apertium-myv-mdf.myv-mdf.t3x"/>
        <file name="myv-mdf.t3x.bin"/>
      </program>
      <program name="hfst-proc $1">
        <file name="myv-mdf.autogen.hfst"/>
      </program>
    </pipeline>
  </mode>

